{
  "name": "pokemon-lexer",
  "version": "1.0.0",
  "description": "Analizador léxico para archivos de equipos Pokémon",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "npm run build && node server.js",
    "dev": "concurrently \"npm run watch\" \"npm run start\"",
    "serve": "npm run build && node server.js",
    "clean": "rimraf public/dist",
    "test": "npm run build && node server.js --port 3000"
  },
  "keywords": ["lexer", "pokemon", "typescript", "compiler", "nodejs"],
  "author": "MarioMerida",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.0",
    "rimraf": "^5.0.0"
  },
  "dependencies": {}
}
